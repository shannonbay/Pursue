# Database (Supabase)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.kdectomewmirjibjzvle.supabase.co:5432/postgres

# JWT
JWT_SECRET=your-super-secret-jwt-key-min-32-chars
JWT_REFRESH_SECRET=your-super-secret-refresh-key-min-32-chars

# Google OAuth
# Web Application Client ID (for web clients)
GOOGLE_CLIENT_ID=your-web-client-id.apps.googleusercontent.com
# Android Client ID (for Android app)
GOOGLE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com

# Consent hash salt (used for ghost-linking consent records on account deletion)
CONSENT_HASH_SALT=your-consent-hash-salt-min-32-chars

# Google Cloud Storage (for progress photos)
# Path to your GCS service account key JSON file
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your-service-account-key.json
# GCS bucket name for storing progress photos (optional, defaults to pursue-photo-journal-australia)
GCS_PHOTO_BUCKET=pursue-photo-journal-australia

# App
NODE_ENV=development
# PORT defaults to 8080 (Cloud Run). Set to 3000 for local dev if preferred.
PORT=3000
FRONTEND_URL=http://localhost:3000

INTERNAL_JOB_KEY=special-internal-job-key-for-cron-jobs-and-background-tasks